        <table:table-row>
          <table:table-cell>
            <text:p>125</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>story.Ch1_MK_Wisteria.item_foot_on_holder.text.d7bf6dc2bd67d073e73d0dfec83bf1471929e0fd</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>events.Ch1_MK_Wisteria.item_foot_on_holder.dlg.chara2</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>chara2</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>You're just a filthy witch!</text:p>
          </table:table-cell>
        </table:table-row>
        <table:table-row>
          <table:table-cell>
            <text:p>126</text:p>
          </table:table-cell>
          <table:table-cell>

        <table:table-row>
          <table:table-cell>
            <text:p>93</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>story.Ch1_MK_Wisteria.item_compress_on_holder.text.a2f7e82772dad7231f65d425e54fb8c34712ddbc</text:p>

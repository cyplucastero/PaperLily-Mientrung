          <table:table-cell>
            <text:p>events.Ch1_MK_Wisteria.item_foot_on_holder.dlg.chara2</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>chara2</text:p>
          </table:table-cell>
          <table:table-cell>
            <text:p>I'm reporting you, you won't be needing this!</text:p>
          </table:table-cell>
        </table:table-row>
        <table:table-row>

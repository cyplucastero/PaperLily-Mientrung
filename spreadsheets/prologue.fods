          </table:table-cell>
        </table:table-row>
        <table:table-row>
          <table:table-cell table:formula="of:=IFERROR(VLOOKUP(&quot;system.cg.prologue_instructions_need_6&quot;;[$Base.$A:.$B];1;0);&quot;&quot;)"></table:table-cell>
          <table:table-cell table:formula="of:=IFERROR(VLOOKUP(&quot;system.cg.prologue_instructions_need_6&quot;;[$Base.$A:.$B];2;0);&quot;&quot;)"></table:table-cell>
